<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" Width="400" Height="100" HorizontalAlignment="Center" VerticalAlignment="Center" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="NrpaNup.MainWindow"
        x:CompileBindings="True"
        Background="#262626"
        WindowStartupLocation="CenterScreen"
        Title="NrpaNup">
        <StackPanel>
            <Label Content="ZLB " HorizontalAlignment="Center" VerticalAlignment="Center"  FontSize="16" IsVisible="True"/>
            <Label Content="Zlata Banka Slovenije" HorizontalAlignment="Center" VerticalAlignment="Center" IsVisible="True"/>
            <Button x:Name="LoginButton" Content="Prijava" CornerRadius="10" Margin="3" Padding="5" 
                    HorizontalAlignment="Center" VerticalAlignment="Center" Click="LoginButton_OnClick" IsVisible="True"/>
        </StackPanel>
</Window>
