<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="NrpaNup.MainWindow"
        x:CompileBindings="True"
        Background="#262626"
        Title="NrpaNup">
    <Grid ColumnDefinitions="*,*,*" RowDefinitions="*,*,*,*">
        <StackPanel Grid.Row="2" Grid.Column="1">
            <Label Content="Zlata Banka Slovenije ZLB " HorizontalAlignment="Center" VerticalAlignment="Center" IsVisible="True"/>
            <Button x:Name="LoginButton" Content="Login" CornerRadius="10" Margin="3" Padding="5" 
                    HorizontalAlignment="Center" VerticalAlignment="Center" Click="LoginButton_OnClick" IsVisible="True"/>
        </StackPanel>
    </Grid>
</Window>
